{
	"tags"      : {
		"allowUnknownTags" : true
	},
	"plugins"   : ["plugins/markdown"],
        "source"    : {
		"include" : [
                        "../../src/require/gp/",
                        "../../README.md"
		]
	},
	"templates" : {
		"cleverLinks"           : false,
		"monospaceLinks"        : false,
		"dateFormat"            : "ddd MMM Do YYYY",
		"outputSourceFiles"     : true,
		"outputSourcePath"      : true,
		"systemName"            : "Sample Class JS",
		"footer"                : "version 1.0.0",
		"copyright"             : "Geoportail - Copyright (c) IGN, released under the BSD license",
		"navType"               : "inline",
		"theme"                 : "darkly",
		"linenums"              : true,
		"collapseSymbols"       : true,
		"inverseNav"            : true,
		"highlightTutorialCode" : false,
		"syntaxTheme"           : "default"
	},
	"markdown"  : {
		"parser"   : "gfm",
		"hardwrap" : true
	},
        "opts": {
            "template": "node_modules/ink-docstrap/template/",
            "template-inactif": "node_modules/jaguarjs-jsdoc/",
            "template-inactif": "node_modules/jsdoc-oblivion/template/",
            "template-inactif": "node_modules/jsdoc-dash-template",
            "encoding": "utf8",
            "destination": "../build/",
            "recurse": true,
            "lenient": true,
	    "verbose": true,
            "tutorials": "./tutorials"
        }
}
